#include "stdafx.h"
#include <conio.h>
#include <iostream> 

using namespace std;

void main()

{
	
	float A,R;
	cout << "Ingrese el radio de la circunferencia:"<<endl;
	cin >> R;
	A=3.1416*R*R;
	cout << "El area de la circunferencia: "<<A;

	getch();
}
