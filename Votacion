#include "conio.h"

using namespace std;

void main()
{ 
    int e;
	cout<<"Ingrese la edad : ";
	cin>> e;
	if (e>=18)
		cout<<"Puede votar";
	else
		cout<<"No puede votar";

	getch();
}
